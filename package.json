{
  "name": "autocommit",
  "displayName": "autocommit",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autocommit.gitHook",
        "title": "Setup Githook"
      },
      {
        "command": "autocommit.apiKey",
        "title": "Setup Api Key"
      }
    ],
    "configuration": {
      "title": "AutoCommit",
      "properties": {
        "autocommit.apiConfiguration.model": {
          "type": "string",
          "default": "gpt-4o-mini",
          "description": "This configuration set the OpenAI model used for context gathering and commit message generation."
        },
        "autocommit.apiConfiguration.apiKey": {
          "type": "number",
          "default": 0.5,
          "description": "This configuration set the temperature of the GPT model."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}